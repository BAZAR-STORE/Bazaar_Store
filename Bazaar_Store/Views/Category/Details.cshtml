@addTagHelper *, Microsoft.AspNetCore.Mvc.TagHelpers
@using Bazaar_Store.Models
@model Category

<div>
    <h4>@Model.Name</h4>
    <hr />
  
 
    
    <table  class="table">
    <thead>
        <tr>
            <th>
                Id
            </th>
            <th>
                Name
            </th>
            <th>
                Price
            </th>
            <th>
                Description
            </th>
             <th>
               Barcode
            </th> 
            <th>
               Image
            </th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model.ProdectList) {
     <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Id)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Price)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Description)
            </td>
             <td>
                @item.BarCode
            </td>
            <td>
                @item.URL
            </td>
     </tr>
        }
    </tbody>
    </table>

</div>