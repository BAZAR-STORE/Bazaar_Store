@using Bazaar_Store.Models
@model List<Product>
@addTagHelper *, Microsoft.AspNetCore.Mvc.TagHelpers

<h4> Category</h4>

<ul>
        @foreach(var item in Model)
    {
        <li>
            <a asp-controller="Home" asp-action="Index" asp-route-name=@item.Name asp-route-job=@item.Price>@item.Name</a>
           <!-- <a href="/Home/Index/?name=@item.Name&job=@item.Price">@item.Name</a> -->
        </li>
        <!--<li>Name: @item.Name , Job: @item.Price</li>-->
    }
</ul>